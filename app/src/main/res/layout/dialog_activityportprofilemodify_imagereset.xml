<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="303dp"
    android:layout_height="189dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/tab1_dialog_layout_round"
    android:layout_gravity="center"
    >
    <TextView
        android:id="@+id/port_profile_modify_image_reset_dialog_title2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="기본 이미지로 변경됩니다."
        android:layout_marginTop="54dp"
        android:textSize="14sp"
        android:textColor="#010101"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />
    <Button
        android:id="@+id/port_profile_modify_image_reset_dialog_cancle_bt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/my_port_modify_ok_bt"
        android:text="취소"
        android:textSize="13sp"
        android:textColor="@color/normal_title_color"
        android:paddingLeft="54dp"
        android:paddingRight="54dp"
        android:paddingTop="15dp"
        android:paddingBottom="15dp"
        app:layout_constraintTop_toBottomOf="@+id/port_profile_modify_image_reset_dialog_title2"
        app:layout_constraintEnd_toStartOf="@+id/port_profile_modify_image_reset_dialog_space"
        android:layout_marginTop="45dp"
        />
    <Space
        android:id="@+id/port_profile_modify_image_reset_dialog_space"
        android:layout_width="6dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/port_profile_modify_image_reset_dialog_title2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="45dp"
        />
    <Button
        android:id="@+id/port_profile_modify_image_reset_dialog_ok_bt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/tab1_dialog_round_ok_bt"
        android:text="확인"
        android:textSize="13sp"
        android:textColor="@color/normal_title_color"
        android:paddingLeft="54dp"
        android:paddingRight="54dp"
        android:paddingTop="15dp"
        android:paddingBottom="15dp"
        app:layout_constraintTop_toBottomOf="@+id/port_profile_modify_image_reset_dialog_title2"
        app:layout_constraintStart_toEndOf="@+id/port_profile_modify_image_reset_dialog_space"
        android:layout_marginTop="45dp"
        />
</android.support.constraint.ConstraintLayout>