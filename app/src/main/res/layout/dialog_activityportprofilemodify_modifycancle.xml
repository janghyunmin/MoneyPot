<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="303dp"
    android:layout_height="189dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/tab1_dialog_layout_round"
    android:layout_gravity="center"
    >
    <TextView
        android:id="@+id/port_profile_modify_cancle_dialog_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="실행 취소"
        android:textColor="#666666"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="21dp"
        android:textSize="14sp"
        />
    <TextView
        android:id="@+id/port_profile_modify_cancle_dialog_title2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="수정된 내용이 사라집니다."
        android:layout_marginTop="17dp"
        android:textSize="14sp"
        android:textColor="#010101"
        app:layout_constraintTop_toBottomOf="@+id/port_profile_modify_cancle_dialog_title"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />
    <TextView
        android:id="@+id/port_profile_modify_cancle_dialog_title3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="계속 하시겠습니까?"
        android:textSize="14sp"
        android:textColor="#010101"
        app:layout_constraintTop_toBottomOf="@+id/port_profile_modify_cancle_dialog_title2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="5dp"
        />
    <Button
        android:id="@+id/port_profile_modify_cancle_dialog_cancle_bt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/my_port_modify_ok_bt"
        android:text="취소"
        android:textSize="13sp"
        android:textColor="@color/normal_title_color"
        android:paddingLeft="54dp"
        android:paddingRight="54dp"
        android:paddingTop="15dp"
        android:paddingBottom="15dp"
        app:layout_constraintTop_toBottomOf="@+id/port_profile_modify_cancle_dialog_title3"
        app:layout_constraintEnd_toStartOf="@+id/port_profile_modify_cancle_dialog_space"
        android:layout_marginTop="20dp"
        />
    <Space
        android:id="@+id/port_profile_modify_cancle_dialog_space"
        android:layout_width="6dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/port_profile_modify_cancle_dialog_title3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="20dp"
        />
    <Button
        android:id="@+id/port_profile_modify_cancle_dialog_ok_bt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/tab1_dialog_round_ok_bt"
        android:text="확인"
        android:textSize="13sp"
        android:textColor="@color/normal_title_color"
        android:paddingLeft="54dp"
        android:paddingRight="54dp"
        android:paddingTop="15dp"
        android:paddingBottom="15dp"
        app:layout_constraintTop_toBottomOf="@+id/port_profile_modify_cancle_dialog_title3"
        app:layout_constraintStart_toEndOf="@+id/port_profile_modify_cancle_dialog_space"
        android:layout_marginTop="20dp"
        />
</androidx.constraintlayout.widget.ConstraintLayout>