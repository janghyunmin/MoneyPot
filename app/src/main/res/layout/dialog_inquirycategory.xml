<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:layout_marginLeft="29dp"
    android:layout_marginRight="29dp"
    android:background="@drawable/rectangle_category"
    >

    <TextView
        android:id="@+id/appBt"
        android:layout_width="match_parent"
        android:layout_height="61dp"
        android:text="앱 이용 문의"
        android:textColor="@color/text_black_color"
        android:textSize="16dp"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:id="@+id/accountBt"
        android:layout_width="match_parent"
        android:layout_height="61dp"
        android:text="계좌 관련 문의"
        android:textColor="@color/text_black_color"
        android:textSize="16dp"
        app:layout_constraintTop_toBottomOf="@+id/appBt"
        android:gravity="center"
        />

    <TextView
        android:id="@+id/productBt"
        android:layout_width="match_parent"
        android:layout_height="61dp"
        android:text="상품 관련 문의"
        android:textColor="@color/text_black_color"
        android:textSize="16dp"
        app:layout_constraintTop_toBottomOf="@+id/accountBt"
        android:gravity="center"
        />

    <TextView
        android:id="@+id/yieldBt"
        android:layout_width="match_parent"
        android:layout_height="61dp"
        android:text="수익률 관련 문의"
        android:textColor="@color/text_black_color"
        android:textSize="16dp"
        app:layout_constraintTop_toBottomOf="@+id/productBt"
        android:gravity="center"
        />

    <TextView
        android:id="@+id/otherBt"
        android:layout_width="match_parent"
        android:layout_height="61dp"
        android:text="기타 문의"
        android:textColor="@color/text_black_color"
        android:textSize="16dp"
        app:layout_constraintTop_toBottomOf="@+id/yieldBt"
        android:gravity="center"
        />

</androidx.constraintlayout.widget.ConstraintLayout>