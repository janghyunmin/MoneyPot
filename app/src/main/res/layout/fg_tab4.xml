            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="7dp"
            android:layout_marginStart="24dp"
            />

        <View
            android:id="@+id/myInfoB"
            android:layout_width="97dp"
            android:layout_height="34dp"
            android:background="@drawable/custom_bt_4dp"
            app:layout_constraintTop_toTopOf="@+id/myInfoTab"
            app:layout_constraintBottom_toBottomOf="@+id/myInfoTab"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="10dp"
            />
        <TextView
            android:id="@+id/myInfoTab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="내 정보"
            android:textSize="13dp"
            android:textColor="@color/c_ffffff"
            app:layout_constraintTop_toBottomOf="@+id/image"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="24dp"
            android:layout_marginTop="12dp"
            />
        <ImageView
            android:id="@+id/myInfoImage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/myinfo"
            app:layout_constraintEnd_toStartOf="@+id/containerLayout"
            app:layout_constraintTop_toTopOf="@+id/myInfoTab"
            app:layout_constraintBottom_toBottomOf="@+id/myInfoTab"
            />


        <View
            android:id="@+id/centerB"
            android:layout_width="97dp"
            android:layout_height="34dp"
            android:background="@drawable/custom_bt_4dp"
            app:layout_constraintTop_toTopOf="@+id/centerTab"
            app:layout_constraintBottom_toBottomOf="@+id/centerTab"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="10dp"
            android:visibility="gone"
            />
        <TextView
            android:id="@+id/centerTab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="고객센터"
            app:layout_constraintTop_toBottomOf="@+id/myInfoTab"
