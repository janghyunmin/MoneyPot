            android:layout_marginTop="31dp"
            android:layout_marginStart="24dp"
            android:textSize="13dp"
            android:textColor="@color/c_9a9a9a"
            />
        <ImageView
            android:id="@+id/pushImage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/pushalert"
            app:layout_constraintEnd_toStartOf="@+id/containerLayout"
            app:layout_constraintTop_toTopOf="@+id/pushTab"
            app:layout_constraintBottom_toBottomOf="@+id/pushTab"
            android:visibility="gone"
            />

