            app:layout_constraintEnd_toStartOf="@+id/containerLayout"
            app:layout_constraintTop_toTopOf="@+id/noticeTab"
            app:layout_constraintBottom_toBottomOf="@+id/noticeTab"
            android:visibility="gone"
            />

        <View
            android:id="@+id/pushB"
            android:layout_width="97dp"
            android:layout_height="34dp"
            android:background="@drawable/custom_bt_4dp"
            app:layout_constraintTop_toTopOf="@+id/pushTab"
