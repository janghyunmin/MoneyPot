<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="312dp"
    android:layout_height="224dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/card_view"
    >


    <TextView
        android:id="@+id/topTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="해외기업 차트"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:textSize="13dp"
        android:textColor="@color/c_3d3f42"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="전일 종가 기준"
        app:layout_constraintStart_toEndOf="@+id/topTitle"
        app:layout_constraintTop_toTopOf="@+id/topTitle"
        app:layout_constraintBottom_toBottomOf="@+id/topTitle"
        android:layout_marginStart="3dp"
        android:textSize="12dp"
        android:textColor="@color/c_9a9a9a"
        />

    <ImageView
        android:id="@+id/image1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_sector_coffee"
        app:layout_constraintTop_toBottomOf="@+id/topTitle"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="19dp"
        android:layout_marginStart="16dp"
        />

    <View
        android:id="@+id/line1"
        android:layout_width="1dp"
        android:layout_height="1dp"
        app:layout_constraintTop_toTopOf="@+id/image1"
        app:layout_constraintBottom_toBottomOf="@+id/image1"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <TextView
        android:id="@+id/num1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:textSize="12dp"
        android:textColor="@color/c_4e7cff"
        app:layout_constraintTop_toTopOf="@+id/image1"
        app:layout_constraintBottom_toBottomOf="@+id/image1"
        app:layout_constraintStart_toEndOf="@+id/image1"
        android:layout_marginStart="13dp"
        />

    <TextView
        android:id="@+id/title1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="스타벅스"
        android:textSize="13dp"
        android:textColor="@color/c_3d3f42"
        app:layout_constraintTop_toTopOf="@+id/num1"
        app:layout_constraintBottom_toBottomOf="@+id/num1"
        app:layout_constraintStart_toEndOf="@+id/num1"
        android:layout_marginStart="8dp"
        />

    <TextView
        android:id="@+id/rate1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="11.02%"
        app:layout_constraintBottom_toTopOf="@+id/line1"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="16dp"
        android:textSize="13dp"
        android:textColor="@color/c_f02654"
        />
    <TextView
        android:id="@+id/price1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="292,030원"
        app:layout_constraintTop_toBottomOf="@+id/line1"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="16dp"
        android:textSize="12dp"
        android:textColor="@color/c_9a9a9a"
        />

    <ImageView
        android:id="@+id/image2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_sector_coffee"
        app:layout_constraintTop_toBottomOf="@+id/image1"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="17dp"
        android:layout_marginStart="16dp"
        />

    <View
        android:id="@+id/line2"
        android:layout_width="1dp"
        android:layout_height="1dp"
        app:layout_constraintTop_toTopOf="@+id/image2"
        app:layout_constraintBottom_toBottomOf="@+id/image2"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <TextView
        android:id="@+id/num2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2"
        android:textSize="12dp"
        android:textColor="@color/c_9a9a9a"
        app:layout_constraintTop_toTopOf="@+id/image2"
        app:layout_constraintBottom_toBottomOf="@+id/image2"
        app:layout_constraintStart_toEndOf="@+id/image2"
        android:layout_marginStart="13dp"
        />

    <TextView
        android:id="@+id/title2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="애브비"
        android:textSize="13dp"
        android:textColor="@color/c_3d3f42"
        app:layout_constraintTop_toTopOf="@+id/num2"
        app:layout_constraintBottom_toBottomOf="@+id/num2"
        app:layout_constraintStart_toEndOf="@+id/num2"
        android:layout_marginStart="8dp"
        />
    <TextView
        android:id="@+id/rate2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="11.02%"
        app:layout_constraintBottom_toTopOf="@+id/line2"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="16dp"
        android:textSize="13dp"
        android:textColor="@color/c_f02654"
        />
    <TextView
        android:id="@+id/price2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="292,030원"
        app:layout_constraintTop_toBottomOf="@+id/line2"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="16dp"
        android:textSize="12dp"
        android:textColor="@color/c_9a9a9a"
        />

    <ImageView
        android:id="@+id/image3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_sector_coffee"
        app:layout_constraintTop_toBottomOf="@+id/image2"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="17dp"
        android:layout_marginStart="16dp"
        />

    <View
        android:id="@+id/line3"
        android:layout_width="1dp"
        android:layout_height="1dp"
        app:layout_constraintTop_toTopOf="@+id/image3"
        app:layout_constraintBottom_toBottomOf="@+id/image3"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <TextView
        android:id="@+id/num3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3"
        android:textSize="12dp"
        android:textColor="@color/c_9a9a9a"
        app:layout_constraintTop_toTopOf="@+id/image3"
        app:layout_constraintBottom_toBottomOf="@+id/image3"
        app:layout_constraintStart_toEndOf="@+id/image3"
        android:layout_marginStart="13dp"
        />

    <TextView
        android:id="@+id/title3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="마이크론테크"
        android:textSize="13dp"
        android:textColor="@color/c_3d3f42"
        app:layout_constraintTop_toTopOf="@+id/num3"
        app:layout_constraintBottom_toBottomOf="@+id/num3"
        app:layout_constraintStart_toEndOf="@+id/num3"
        android:layout_marginStart="8dp"
        />

    <TextView
        android:id="@+id/rate3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="11.02%"
        app:layout_constraintBottom_toTopOf="@+id/line3"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="16dp"
        android:textSize="13dp"
        android:textColor="@color/c_f02654"
        />
    <TextView
        android:id="@+id/price3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="292,030원"
        app:layout_constraintTop_toBottomOf="@+id/line3"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="16dp"
        android:textSize="12dp"
        android:textColor="@color/c_9a9a9a"
        />

    <View
        android:layout_width="match_parent"
        android:layout_height="42dp"
        app:layout_constraintTop_toBottomOf="@+id/image3"
        />
</androidx.constraintlayout.widget.ConstraintLayout>