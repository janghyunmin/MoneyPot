le="bold"--><!--/>--><!--<Button--><!--android:id="@+id/tab4_recyclerView_chart_share_bt"--><!--android:layout_width="77dp"--><!--android:layout_height="38.5dp"--><!--android:layout_gravity="center_vertical"--><!--android:layout_marginLeft="31dp"--><!--android:textSize="13sp"--><!--android:text="공유"--><!--android:textColor="@color/chart_bt_normal_color"--><!--android:background="@drawable/chart_add_view_bt_round"--><!--android:gravity="center"--><!--/>--><!--</LinearLayout>--><!--</RelativeLayout>-->

