<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">

    <androidx.constraintlayout.widget.ConstraintLayout

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginStart="14dp"
        >
        <TextView
            android:id="@+id/title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="투자 경험이 있는 상품을 선택해주세요. (중복 선택 가능)"
            android:background="@drawable/item_propensity_yearinput1"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:paddingStart="20dp"
            android:paddingEnd="20dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:textSize="14dp"
            android:textColor="@color/text_black_color"
            android:maxWidth="282dp"
            android:lineSpacingMultiplier="1.2"
            />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/oneLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/item_propensity_yearinput2"
            app:layout_constraintTop_toBottomOf="@+id/title"
            app:layout_constraintStart_toStartOf="@+id/title"
            app:layout_constraintEnd_toEndOf="@+id/title"
            android:orientation="horizontal"
            >
            <ImageView
                android:id="@+id/stockImage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_checkbox_inactive_24_dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/stockText"
                app:layout_constraintBottom_toBottomOf="@+id/stockText"
                android:layout_marginStart="17dp"
                />
            <TextView
                android:id="@+id/stockText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="주식 신용 거래"
                android:textSize="12dp"
                android:textColor="@color/text_black_color"
                android:maxWidth="180dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:layout_marginStart="6dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/stockImage"
                android:lineSpacingMultiplier="1.3"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/twoLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/item_propensity_yearinput2"
            app:layout_constraintTop_toBottomOf="@+id/oneLayout"
            app:layout_constraintStart_toStartOf="@+id/title"
            app:layout_constraintEnd_toEndOf="@+id/title"
            android:orientation="horizontal"
            >
            <ImageView
                android:id="@+id/warpImage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_checkbox_inactive_24_dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/warpText"
                app:layout_constraintBottom_toBottomOf="@+id/warpText"
                android:layout_marginStart="17dp"
                />
            <TextView
                android:id="@+id/warpText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="투자 자문 / 일임(wrap)"
                android:textSize="12dp"
                android:textColor="@color/text_black_color"
                android:maxWidth="180dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:layout_marginStart="6dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/warpImage"
                android:lineSpacingMultiplier="1.3"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/threeLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/item_propensity_yearinput2"
            app:layout_constraintTop_toBottomOf="@+id/twoLayout"
            app:layout_constraintStart_toStartOf="@+id/title"
            app:layout_constraintEnd_toEndOf="@+id/title"
            android:orientation="horizontal"
            >
            <ImageView
                android:id="@+id/mixImage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_checkbox_inactive_24_dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/mixText"
                app:layout_constraintBottom_toBottomOf="@+id/mixText"
                android:layout_marginStart="17dp"
                />
            <TextView
                android:id="@+id/mixText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="채권 / 혼합형 펀드"
                android:textSize="12dp"
                android:textColor="@color/text_black_color"
                android:maxWidth="180dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:layout_marginStart="6dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/mixImage"
                android:lineSpacingMultiplier="1.3"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/foreLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/item_propensity_yearinput2"
            app:layout_constraintTop_toBottomOf="@+id/threeLayout"
            app:layout_constraintStart_toStartOf="@+id/title"
            app:layout_constraintEnd_toEndOf="@+id/title"
            android:orientation="horizontal"
            >
            <ImageView
                android:id="@+id/fundImage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_checkbox_inactive_24_dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/fundText"
                app:layout_constraintBottom_toBottomOf="@+id/fundText"
                android:layout_marginStart="17dp"
                />
            <TextView
                android:id="@+id/fundText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="주식형 펀드"
                android:textSize="12dp"
                android:textColor="@color/text_black_color"
                android:maxWidth="180dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:layout_marginStart="6dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/fundImage"
                android:lineSpacingMultiplier="1.3"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/fiveLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/item_propensity_yearinput2"
            app:layout_constraintTop_toBottomOf="@+id/foreLayout"
            app:layout_constraintStart_toStartOf="@+id/title"
            app:layout_constraintEnd_toEndOf="@+id/title"
            android:orientation="horizontal"
            >
            <ImageView
                android:id="@+id/outImage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_checkbox_inactive_24_dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/outText"
                app:layout_constraintBottom_toBottomOf="@+id/outText"
                android:layout_marginStart="17dp"
                />
            <TextView
                android:id="@+id/outText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="외화 증권"
                android:textSize="12dp"
                android:textColor="@color/text_black_color"
                android:maxWidth="180dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:layout_marginStart="6dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/outImage"
                android:lineSpacingMultiplier="1.3"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/sixLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/item_propensity_yearinput2"
            app:layout_constraintTop_toBottomOf="@+id/fiveLayout"
            app:layout_constraintStart_toStartOf="@+id/title"
            app:layout_constraintEnd_toEndOf="@+id/title"
            android:orientation="horizontal"
            >
            <ImageView
                android:id="@+id/optionImage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_checkbox_inactive_24_dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/optionText"
                app:layout_constraintBottom_toBottomOf="@+id/optionText"
                android:layout_marginStart="17dp"
                />
            <TextView
                android:id="@+id/optionText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="선물 / 옵션"
                android:textSize="12dp"
                android:textColor="@color/text_black_color"
                android:maxWidth="180dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:layout_marginStart="6dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/optionImage"
                android:lineSpacingMultiplier="1.3"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/sevenLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/item_propensity_yearinput2"
            app:layout_constraintTop_toBottomOf="@+id/sixLayout"
            app:layout_constraintStart_toStartOf="@+id/title"
            app:layout_constraintEnd_toEndOf="@+id/title"
            android:orientation="horizontal"
            >
            <ImageView
                android:id="@+id/trustImage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_checkbox_inactive_24_dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/trustText"
                app:layout_constraintBottom_toBottomOf="@+id/trustText"
                android:layout_marginStart="17dp"
                />
            <TextView
                android:id="@+id/trustText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="신탁"
                android:textSize="12dp"
                android:textColor="@color/text_black_color"
                android:maxWidth="180dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:layout_marginStart="6dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/trustImage"
                android:lineSpacingMultiplier="1.3"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/eightLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/item_propensity_yearinput2"
            app:layout_constraintTop_toBottomOf="@+id/sevenLayout"
            app:layout_constraintStart_toStartOf="@+id/title"
            app:layout_constraintEnd_toEndOf="@+id/title"
            android:orientation="horizontal"
            >
            <ImageView
                android:id="@+id/bondImage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_checkbox_inactive_24_dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/bondText"
                app:layout_constraintBottom_toBottomOf="@+id/bondText"
                android:layout_marginStart="17dp"
                />
            <TextView
                android:id="@+id/bondText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="채권"
                android:textSize="12dp"
                android:textColor="@color/text_black_color"
                android:maxWidth="180dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:layout_marginStart="6dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/bondImage"
                android:lineSpacingMultiplier="1.3"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/nineLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/item_propensity_yearinput2"
            app:layout_constraintTop_toBottomOf="@+id/eightLayout"
            app:layout_constraintStart_toStartOf="@+id/title"
            app:layout_constraintEnd_toEndOf="@+id/title"
            android:orientation="horizontal"
            >
            <ImageView
                android:id="@+id/elwImage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_checkbox_inactive_24_dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/elwText"
                app:layout_constraintBottom_toBottomOf="@+id/elwText"
                android:layout_marginStart="17dp"
                />
            <TextView
                android:id="@+id/elwText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ELW"
                android:textSize="12dp"
                android:textColor="@color/text_black_color"
                android:maxWidth="180dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:layout_marginStart="6dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/elwImage"
                android:lineSpacingMultiplier="1.3"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/okText"
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:text="확인"
            android:textSize="12dp"
            android:textColor="@color/text_white_color"
            android:lineSpacingMultiplier="1.3"
            app:layout_constraintTop_toTopOf="@+id/tenLayout"
            app:layout_constraintStart_toStartOf="@+id/title"
            app:layout_constraintEnd_toEndOf="@+id/title"
            android:gravity="center"
            android:background="#528139"
            />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/tenLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/item_propensity_yearinput2"
            app:layout_constraintTop_toBottomOf="@+id/nineLayout"
            app:layout_constraintStart_toStartOf="@+id/title"
            app:layout_constraintEnd_toEndOf="@+id/title"
            android:orientation="horizontal"
            >
            <ImageView
                android:id="@+id/elsImage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_checkbox_inactive_24_dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/elsUpText"
                app:layout_constraintBottom_toBottomOf="@+id/elsUpText"
                android:layout_marginStart="17dp"
                />
            <TextView
                android:id="@+id/elsUpText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="원금 비보장형 ELS / DLS / ELF"
                android:textSize="12dp"
                android:textColor="@color/text_black_color"
                android:maxWidth="180dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:layout_marginStart="6dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/elsImage"
                android:lineSpacingMultiplier="1.3"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <View
            android:layout_width="0dp"
            android:layout_height="50dp"
            app:layout_constraintTop_toBottomOf="@+id/tenLayout"
            app:layout_constraintStart_toStartOf="@+id/title"
            app:layout_constraintEnd_toEndOf="@+id/title"/>

    </androidx.constraintlayout.widget.ConstraintLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"/>
</LinearLayout>