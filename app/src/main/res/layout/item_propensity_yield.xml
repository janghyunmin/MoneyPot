<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginStart="14dp"
        >
        <TextView
            android:id="@+id/title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="투자를 통한 수익률을 얼마나 기대하세요?"
            android:background="@drawable/item_propensity_yearinput1"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:paddingStart="20dp"
            android:paddingEnd="20dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:textSize="14dp"
            android:textColor="@color/c_3d3f42"
            android:maxWidth="290dp"
            android:lineSpacingMultiplier="1.2"
            />
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/oneLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/item_propensity_yearinput2"
            app:layout_constraintTop_toBottomOf="@+id/title"
            app:layout_constraintStart_toStartOf="@+id/title"
            app:layout_constraintEnd_toEndOf="@+id/title"
            android:orientation="horizontal"
            >
            <ImageView
                android:id="@+id/oneImage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_checkbox_inactive_24_dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/oneText"
                app:layout_constraintBottom_toBottomOf="@+id/oneText"
                android:layout_marginStart="17dp"
                />
            <TextView
                android:id="@+id/oneText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="은행이자 수준의 수익 기대\n(제한적 손실 감수)"
                android:textSize="12dp"
                android:textColor="@color/c_3d3f42"
                android:maxWidth="2000dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:layout_marginStart="6dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/oneImage"
                android:lineSpacingMultiplier="1.3"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/twoLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/item_propensity_yearinput2"
            app:layout_constraintTop_toBottomOf="@+id/oneLayout"
            app:layout_constraintStart_toStartOf="@+id/title"
            app:layout_constraintEnd_toEndOf="@+id/title"
            android:orientation="horizontal"
            >
            <ImageView
                android:id="@+id/twoImage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_checkbox_inactive_24_dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/twoText"
                app:layout_constraintBottom_toBottomOf="@+id/twoText"
                android:layout_marginStart="17dp"
                />
            <TextView
                android:id="@+id/twoText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="은행이자보다 높은 수익 기대\n(약간의 원금 손실 감수)"
                android:textSize="12dp"
                android:textColor="@color/c_3d3f42"
                android:maxWidth="200dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:layout_marginStart="6dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/twoImage"
                android:lineSpacingMultiplier="1.3"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/threeLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/item_propensity_yearinput2"
            app:layout_constraintTop_toBottomOf="@+id/twoLayout"
            app:layout_constraintStart_toStartOf="@+id/title"
            app:layout_constraintEnd_toEndOf="@+id/title"
            android:orientation="horizontal"
            >
            <ImageView
                android:id="@+id/threeImage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_checkbox_inactive_24_dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/threeText"
                app:layout_constraintBottom_toBottomOf="@+id/threeText"
                android:layout_marginStart="17dp"
                />
            <TextView
                android:id="@+id/threeText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="시장 수익률 수준의 수익 기대\n(원금 손실 감수)"
                android:textSize="12dp"
                android:textColor="@color/c_3d3f42"
                android:maxWidth="200dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:layout_marginStart="6dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/threeImage"
                android:lineSpacingMultiplier="1.3"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/foreLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/item_propensity_yearinput3"
            app:layout_constraintTop_toBottomOf="@+id/threeLayout"
            app:layout_constraintStart_toStartOf="@+id/title"
            app:layout_constraintEnd_toEndOf="@+id/title"
            android:orientation="horizontal"
            >
            <ImageView
                android:id="@+id/foreImage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_checkbox_inactive_24_dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/foreText"
                app:layout_constraintBottom_toBottomOf="@+id/foreText"
                android:layout_marginStart="17dp"
                />
            <TextView
                android:id="@+id/foreText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="시장 수익률보다 높은 수익률 기대\n(원금보다 큰 손실도 감수)"
                android:textSize="12dp"
                android:textColor="@color/c_3d3f42"
                android:maxWidth="200dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:layout_marginStart="6dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/foreImage"
                android:lineSpacingMultiplier="1.3"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <View
        android:layout_width="wrap_content"
        android:layout_height="10dp"/>
</LinearLayout>